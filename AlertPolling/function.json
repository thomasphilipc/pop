{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */1 * * * *"
    },
    {
      "type": "table",
      "direction": "in",
      "name": "clientTable",
      "tableName": "clientTable",
      "take": "50",
      "connection": "AZURE_STORAGE_CONNECTION_STRING"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "msg",
      "queueName": "alertsqueue",
      "connection": "AZURE_STORAGE_CONNECTION_STRING"
    }
  ]
}