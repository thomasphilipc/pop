{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "type": "table",
      "direction": "in",
      "name": "clientsJSON",
      "tableName": "clientTable",
      "partitionKey": "master",
      "take": "50",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "msg",
      "direction": "out",
      "type": "queue",
      "queueName": "masterhandlerqueue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
